<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.done"
    android:targetSandboxVersion="1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".AccountConnected"></activity>
        <activity android:name=".ProAccountActivity" />
        <activity android:name=".joinDoc.BestProfileActivity" />
        <activity android:name=".PersonalInformationActivity" />
        <activity android:name=".joinDoc.AwayFromActivity" />
        <activity android:name=".joinDoc.JoinAsActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ServiceDetailsActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".SearchServicesActivity" />
        <activity android:name=".VideoAnimationActivity" />
        <activity android:name=".GraphicActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>